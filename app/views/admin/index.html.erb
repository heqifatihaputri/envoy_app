<p id="notice"><%= notice %></p>

<h1>All Admin</h1>

<table>
  <thead>
    <tr>
      <th>Full Name</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.full_name %></td>
        <td><%= user.role_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

